{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "Exchange": "rockbot"
  },
  "ScriptBridge": {
    "AgentName": "script-bridge",
    "DefaultResultTopic": "script.result"
  },
  "Scripts": {
    "Container": {
      "Namespace": "rockbot-scripts",
      "Image": "python:3.12-slim",
      "CpuLimit": "500m",
      "MemoryLimit": "256Mi"
    }
  }
}
